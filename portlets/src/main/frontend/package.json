{
  "name": "Gamification",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "//": "Option --progress: print compilation progress in percentage",
    "clean": "npm cache clean --force",
    "start": "webpack-dashboard webpack-dev-server -d --hot --config webpack.dev.js --watch",
    "dev": "webpack --mode development --config webpack.dev.js",
    "watch": "webpack --config webpack.dev.js --progress --colors --watch -d",
    "build": "webpack -p --progress --trace-deprecation --config webpack.prod.js"
  },
  "author": "eXo",
  "license": "LGPL",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.13.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.0.0-rc.8",
    "jquery": "^3.2.1",
    "vue": "^2.5.16",
    "vue-avatar": "^2.1.4",
    "vue-bootstrap-datetimepicker": "^4.1.3",
    "vue-d2b": "^1.0.11",
    "vue-moment": "^3.2.0",
    "vue-table-component": "^1.9.2",
  }
}
